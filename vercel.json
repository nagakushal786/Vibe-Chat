{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server/api/$1"
      }
    ],
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ]
  }
  